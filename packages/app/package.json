  },
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "pnpm run build --watch"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.1.2",
    "@rollup/pluginutils": "^4.1.1",
    "@types/lru-cache": "^5.1.1",
    "esbuild": "^0.14.47",
    "globby": "^13.1.1",
    "gray-matter": "^4.0.3",
    "hast-util-to-html": "^8.0.3",
    "js-yaml": "^4.1.0",
    "lru-cache": "^6.0.0",
    "magic-string": "^0.25.9",
    "node-fetch": "^3.2.4",
    "ora": "^6.0.0",
    "pkg-up": "^2.0.0",
    "set-cookie-parser": "^2.5.1",
    "undici": "^5.10.0",
    "urlpattern-polyfill": "^5.0.0"
