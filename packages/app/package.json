      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@wooorm/starry-night": "^1.0.0",
    "fast-glob": "^3.2.11",
    "kleur": "^4.1.4",
    "rollup": "^2.58.0",
    "shiki": "^0.10.1",
    "tsup": "^6.2.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.0"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "app",
    "docs",
    "edge",
    "esm",
    "fast",
    "lightweight",
    "modern",
    "nextjs",
    "ssg",
    "ssr",
    "vercel",
    "vite",
    "vessel"
