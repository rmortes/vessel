{
  "name": "@vessel-js/app",
  "version": "0.1.0",
  "type": "module",
  "types": "index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vessel-js/vessel.git",
    "directory": "packages/app"
  },
  "bugs": {
    "url": "https://github.com/vessel-js/vessel/issues"
  },
  "files": [
    "dist/",
    "index.d.ts",
    "globals.d.ts",
    "node.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./dist/client.js"
